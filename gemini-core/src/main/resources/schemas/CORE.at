#  Gemini Core Module

INTERFACE Domain {
    TEXT code *
}

ENTITY Entity {
    TEXT name *
    TEXT module
    TEXT displayName
}

ENTITY Field {
    TEXT    name *
    Entity  entity *
    TEXT    type
    Entity  refEntity
    BOOL    isLogicalKey
}

ENTITY ValueStrategy IMPLEMENTS Domain

ENTITY FieldResolution {
    Field   field *
    TEXT    code *
    TEXT    value
}

ENTITY InitRecord {
    Entity  entity          *
    TEXT    versionName     *
    LONG    versionNumber   *
}


